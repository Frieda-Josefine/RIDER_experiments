&amp;#10;        } else {&amp;#10;            if ((dragging === 1)) {&amp;#10;                dragging = 0;&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;if ((moving === 1)) {&amp;#10;    if ((scroll &lt; 0)) {&amp;#10;        scroll = 0;&amp;#10;    } else {&amp;#10;        if ((scroll &gt;= maxScroll)) {&amp;#10;            scroll = maxScroll;&amp;#10;            if ((viewAll === 0)) {&amp;#10;                viewAll = 1;&amp;#10;                continueText_3.text = &quot; \n \n Press space to agree to these conditions and continue.\n If you do not want to continue feel free to press ecs to exit the experiment&quot;;&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;    scrollText_3.setPos([((- 0.45) * screenRatio), (0.45 + ((scroll * fontSize) * 1.08))]);&amp;#10;    scrollButton_3.setPos([(screenRatio * 0.475), ((trackLength / 2) - ((trackLength * scroll) / maxScroll))]);&amp;#10;    bottomCrop_3.setPos([((- 0.1) + (0.01 * (scroll % 2))), (- 0.475)]);&amp;#10;    text_4.text = ((scroll.toString() + &quot; of &quot;) + maxScroll.toString());&amp;#10;    moving = 0;&amp;#10;}&amp;#10;" valType="extendedCode"/>
