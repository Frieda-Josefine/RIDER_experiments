        <Param name="Begin Experiment" updates="constant" val="ori_list = []" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="ori_list = [];&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="//replace np.linspace with makeArr in JS&amp;#10;directions = [(- 1), 1];&amp;#10;shuffle(directions);&amp;#10;&amp;#10;fixedOri = (startOri * directions[0]);&amp;#10;&amp;#10;minimalDistance = 20;&amp;#10;maxOri = trials.trialList[trials.trialList.length-1].startOri;&amp;#10;oriRange = makeArr(minimalDistance, maxOri, 100);&amp;#10;shuffle(oriRange);&amp;#10;shuffle(directions);&amp;#10;start_distance = (oriRange[0] * directions[0]);&amp;#10;currentDistance = start_distance;&amp;#10;this_ori = (fixedOri + start_distance);&amp;#10;adjustment_startOri = this_ori;&amp;#10;trials.addData(&quot;start_distance&quot;, start_distance);&amp;#10;trials.addData(&quot;fixedOri&quot;, fixedOri);&amp;#10;trials.addData(&quot;minimalDistance&quot;, minimalDistance);&amp;#10;trials.addData(&quot;maxOri&quot;, maxOri);" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="#  set starting val &amp;#10;#set the fixed orientation to be randomly left or right&amp;#10;directions =[-1, 1]&amp;#10;shuffle(directions)&amp;#10;fixedOri = startOri *directions[0]&amp;#10;&amp;#10;#set the adjusmtent stimulus &amp;#10;minimalDistance = 20 # minimal distance to fixedOri&amp;#10;maxOri = trials.trialList[-1]['startOri']&amp;#10;oriRange = np.linspace(minimalDistance, maxOri, 100)&amp;#10;shuffle(oriRange)&amp;#10;shuffle(directions)&amp;#10;start_distance = oriRange[0]*directions[0]&amp;#10;currentDistance = start_distance&amp;#10;this_ori = fixedOri+start_distance&amp;#10;adjustment_startOri = this_ori&amp;#10;&amp;#10;# save information&amp;#10;trials.addData('adjustment_startOri', adjustment_startOri)&amp;#10;trials.addData('start_distance', start_distance)&amp;#10;trials.addData('fixedOri', fixedOri)&amp;#10;trials.addData('minimalDistance', minimalDistance)&amp;#10;trials.addData('maxOri', maxOri)&amp;#10;&amp;#10;&amp;#10;&amp;#10;&amp;#10;" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Both" valType="str"/>
        <Param name="Each Frame" updates="constant" val="# If a button is clicked, adjust the orientation&amp;#10;if adjustMouse.isPressedIn(leftArrow):&amp;#10;    this_ori -=1&amp;#10;elif adjustMouse.isPressedIn(rightArrow):&amp;#10;    this_ori +=1&amp;#10;&amp;#10;&amp;#10;# if the grating is rotated more than one rotation&amp;#10;# clockwise or anticlockwise reset it to its start value&amp;#10;if this_ori &gt;= 360:&amp;#10;    this_ori = 0&amp;#10;elif this_ori &lt;= -360:&amp;#10;    this_ori = 0&amp;#10;&amp;#10;# if the grating is rotated past 180 reset to start ori&amp;#10;if abs(this_ori-adjustment_startOri)&gt;=180:&amp;#10;    this_ori = adjustment_startOri &amp;#10;elif abs(this_ori-adjustment_startOri)&lt;=-180:&amp;#10;    this_ori = adjustment_startOri &amp;#10;&amp;#10;# Keep track of the two distance vectors &amp;#10;# (can be used for teaching if text components enabled)&amp;#10;currentDistance = min(abs(this_ori-fixedOri), abs(abs(this_ori-fixedOri)-180))&amp;#10;currentDistanceA = abs(this_ori-fixedOri)&amp;#10;currentDistanceB = abs(abs(this_ori-fixedOri)-180)" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="//orientations in opposite direction online&amp;#10;if (adjustMouse.isPressedIn(leftArrow)) {&amp;#10;    this_ori += 1;&amp;#10;} else {&amp;#10;    if (adjustMouse.isPressedIn(rightArrow)) {&amp;#10;        this_ori -= 1;&amp;#10;    }&amp;#10;}&amp;#10;if ((this_ori &gt;= 360)) {&amp;#10;    this_ori = 0;&amp;#10;} else {&amp;#10;    if ((this_ori &lt;= (- 360))) {&amp;#10;        this_ori = 0;&amp;#10;    }&amp;#10;}&amp;#10;if ((Math.abs((this_ori - adjustment_startOri)) &gt;= 180)) {&amp;#10;    this_ori = adjustment_startOri;&amp;#10;} else {&amp;#10;    if ((Math.abs((this_ori - adjustment_startOri)) &lt;= (- 180))) {&amp;#10;        this_ori = adjustment_startOri;&amp;#10;    }&amp;#10;}&amp;#10;currentDistance = Math.min(Math.abs((this_ori - fixedOri)), Math.abs((abs((this_ori - fixedOri)) - 180)));&amp;#10;currentDistanceA = Math.abs((this_ori - fixedOri));&amp;#10;currentDistanceB = Math.abs((abs((this_ori - fixedOri)) - 180));" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Routine" updates="constant" val="trials.addData(&quot;this_ori&quot;, this_ori);&amp;#10;accuracy = Math.min(Math.abs((this_ori - fixedOri)), Math.abs((abs((this_ori - fixedOri)) - 180)));&amp;#10;trials.addData(&quot;accuracy&quot;, accuracy);&amp;#10;ori_list.append(accuracy);" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="# save the final orientation state and accuracy (closeness to actual ori)&amp;#10;trials.addData('this_ori', this_ori)&amp;#10;accuracy = min(abs(this_ori-fixedOri), abs(abs(this_ori-fixedOri)-180)) &amp;#10;trials.addData('accuracy', accuracy)&amp;#10;ori_list.append(accuracy)" valType="extendedCode"/>
