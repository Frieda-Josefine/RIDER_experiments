        <Param name="Each JS Frame" updates="constant" val="//orientations in opposite direction online&amp;#10;if (adjustMouse.isPressedIn(leftArrow)) {&amp;#10;    this_ori += 1;&amp;#10;} else {&amp;#10;    if (adjustMouse.isPressedIn(rightArrow)) {&amp;#10;        this_ori -= 1;&amp;#10;    }&amp;#10;}&amp;#10;if ((this_ori &gt;= 360)) {&amp;#10;    this_ori = 0;&amp;#10;} else {&amp;#10;    if ((this_ori &lt;= (- 360))) {&amp;#10;        this_ori = 0;&amp;#10;    }&amp;#10;}&amp;#10;if ((Math.abs((this_ori - adjustment_startOri)) &gt;= 180)) {&amp;#10;    this_ori = adjustment_startOri;&amp;#10;} else {&amp;#10;    if ((Math.abs((this_ori - adjustment_startOri)) &lt;= (- 180))) {&amp;#10;        this_ori = adjustment_startOri;&amp;#10;    }&amp;#10;}&amp;#10;currentDistance = Math.min(Math.abs((this_ori - fixedOri)), Math.abs((abs((this_ori - fixedOri)) - 180)));&amp;#10;currentDistanceA = Math.abs((this_ori - fixedOri));&amp;#10;currentDistanceB = Math.abs((abs((this_ori - fixedOri)) - 180));" valType="extendedCode"/>
