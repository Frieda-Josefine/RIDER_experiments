        <Param name="Begin Experiment" updates="constant" val="img_no = 1&amp;#10;&amp;#10;current_img = 'conditions/instrcutions/instru_' + str(img_no) + '.png'&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="img_no = 1;&amp;#10;current_img = ((&quot;conditions/instrcutions/instru_&quot; + img_no.toString()) + &quot;.png&quot;);&amp;#10;" valType="extendedCode"/>
