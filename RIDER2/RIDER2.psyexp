        <Param name="Begin JS Routine" updates="constant" val="kb = new core.Keyboard({psychoJS: psychoJS, clock: new util.Clock(), waitForStart: true}); &amp;#10;ltm_trial = trial_type;&amp;#10;fixedOri_ltm = startOri_ltm;&amp;#10;&amp;#10;//rotation parameters&amp;#10;keySpeed = 0.5;&amp;#10;increasePerFrame = 0.25;&amp;#10;//set the adjusmtent stimulus &amp;#10;minimalDistance_ltm = 12.5;&amp;#10;maxOri_ltm = 347.5;&amp;#10;&amp;#10;var oriRange_ltm = [11.25, 33.75, 56.25, 78.75, 101.25, 123.75, 146.25, 168.75, 191.25, 213.75, 236.25, 258.75, 281.25, 303.75, 326.25, 348.75];&amp;#10;util.shuffle(oriRange_ltm);&amp;#10;start_distance_ltm = oriRange_ltm[0];&amp;#10;&amp;#10;this_ori_ltm = start_distance_ltm;&amp;#10;&amp;#10;LTMtrial.addData(&quot;start_distance_ltm&quot;, start_distance_ltm);&amp;#10;LTMtrial.addData(&quot;fixedOri_ltm&quot;,fixedOri_ltm);&amp;#10;&amp;#10;LTMtrial.addData(&quot;ltm_trial&quot;, ltm_trial);" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="kb = keyboard.Keyboard()&amp;#10;kb.clock.reset()&amp;#10;#  set starting val &amp;#10;#set the fixed orientation to be randomly left or right&amp;#10;fixedOri = startOri1&amp;#10;keySpeed = 1&amp;#10;increasePerFrame = 0.1&amp;#10;&amp;#10;#set the adjusmtent stimulus &amp;#10;minimalDistance = 12.5 # minimal distance to fixedOri&amp;#10;maxOri = 347.5 # (360-12.5)&amp;#10;oriRange = np.linspace(minimalDistance, maxOri, 16) #create array of possible 16 orientations&amp;#10;shuffle(oriRange)&amp;#10;start_distance = oriRange[0]&amp;#10;currentDistance = start_distance&amp;#10;this_ori = fixedOri+start_distance # &quot;this_ori&quot; determines the orientation the object is shown in at the beginning of the test. The shown rotation will at least be 12.5 degrees different from the memorized orientation (fixed orientation)&amp;#10;adjustment_startOri = this_ori&amp;#10;&amp;#10;# save information&amp;#10;#trials.addData('adjustment_startOri', adjustment_startOri)&amp;#10;#trials.addData('start_distance', start_distance)&amp;#10;#trials.addData('fixedOri', fixedOri)&amp;#10;#trials.addData('minimalDistance', minimalDistance)&amp;#10;#trials.addData('maxOri', maxOri)" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Both" valType="str"/>
        <Param name="Each Frame" updates="constant" val="keys = adjust_keys_LTM.getKeys(waitRelease=False, clear = False)&amp;#10;if len(keys)&gt;0:&amp;#10;    key = keys[len(keys)-1] #get recent key press&amp;#10;    if not key.duration:&amp;#10;        keySpeed += increasePerFrame #This is like acceleration speed so you could think of making some non-linear function&amp;#10;        if keySpeed &gt; 10:&amp;#10;            keySpeed = 1&amp;#10;        if key.name == &quot;left&quot;:&amp;#10;            this_ori += keySpeed&amp;#10;        if key.name == &quot;right&quot;:&amp;#10;            this_ori -= keySpeed&amp;#10;    else: # Meaning PressedKey was released&amp;#10;        keySpeed = 1&amp;#10;        &amp;#10;# if the grating is rotated past 360 reset to start ori&amp;#10;if abs(this_ori-adjustment_startOri)&gt;=360:&amp;#10;    this_ori = adjustment_startOri &amp;#10;elif abs(this_ori-adjustment_startOri)&lt;=-360:&amp;#10;    this_ori = adjustment_startOri " valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="keys = adjust_keys_LTM.getKeys({&quot;waitRelease&quot;: false, &quot;clear&quot;: false});&amp;#10;if ((keys.length &gt; 0)) {&amp;#10;    key = keys[(keys.length - 1)];&amp;#10;    if ((! key.duration)) {&amp;#10;        keySpeed += increasePerFrame;&amp;#10;        if ((keySpeed &gt; 10)) {&amp;#10;            keySpeed = 0.5;&amp;#10;        }&amp;#10;        if ((key.name === &quot;left&quot;)) {&amp;#10;            this_ori_ltm += keySpeed;&amp;#10;        }&amp;#10;        if ((key.name === &quot;right&quot;)) {&amp;#10;            this_ori_ltm -= keySpeed;&amp;#10;        }&amp;#10;    } else {&amp;#10;        keySpeed = 0.5;&amp;#10;    }&amp;#10;}&amp;#10;&amp;#10;&amp;#10;&amp;#10;if ((Math.abs((this_ori_ltm - start_distance_ltm)) &gt;= 360)) {&amp;#10;    this_ori_ltm = start_distance_ltm;&amp;#10;} else {&amp;#10;    if ((Math.abs((this_ori_ltm - start_distance_ltm)) &lt;= (- 360))) {&amp;#10;    this_ori_ltm  = start_distance_ltm;&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Routine" updates="constant" val="LTMtrial.addData(&quot;this_ori_ltm&quot;, this_ori_ltm);&amp;#10;accuracy_ltm = Math.min(Math.abs((this_ori_ltm - fixedOri_ltm)), Math.abs((abs((this_ori_ltm - fixedOri_ltm)) - 360)));&amp;#10;LTMtrial.addData(&quot;accuracy_ltm&quot;, accuracy_ltm);&amp;#10;//ori_list.append(accuracy);" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="# save the final orientation state and accuracy (closeness to actual ori)&amp;#10;#trials.addData('this_ori', this_ori)&amp;#10;#accuracy = min(abs(this_ori-fixedOri), abs(abs(this_ori-fixedOri)-360)) &amp;#10;#trials.addData('accuracy', accuracy)&amp;#10;#ori_list.append(accuracy)" valType="extendedCode"/>
        <Param name="disabled" updates="None" val="False" valType="bool"/>
        <Param name="name" updates="None" val="adjustCode_LTM" valType="code"/>
