        <Param name="Begin Experiment" updates="constant" val="# Counter for the blocks&amp;#10;thisBlock = 0&amp;#10;&amp;#10;&amp;#10;# variable to determine conditions file&amp;#10;main_task_blocks = ((&quot;conditions/conditions_&quot; + str(expInfo[&quot;participant&quot;])) + &quot;.xlsx&quot;)&amp;#10;&amp;#10;# counter variable for accuracy bonus at the end&amp;#10;bonus_accuracies = []" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="thisBlock = 0;&amp;#10;//variable to determine conditions file&amp;#10;input = await fetch(&quot;https://920-rider-3-142.exp.arc.mpib.org/api/inputs/by-group/ids&quot;).then(r =&gt; r.json());&amp;#10;id = input.id;&amp;#10;//id = 4;&amp;#10;psychoJS.experiment.addData(&quot;participant_id&quot;,id);&amp;#10;main_task_blocks = ((&quot;conditions/conditions_&quot; + id.toString()) + &quot;.xlsx&quot;);&amp;#10;" valType="extendedCode"/>
